# Map filename prefixes to beancount accounts and OpenAI prompt types
# Filename format: "Account Name YYMM.pdf" (e.g., "Amex 2501.pdf")

Amex:
  beancount_account: "Liabilities:CreditCards:Amex"
  openai_prompt_type: default

Bank Checking:
  beancount_account: "Assets:Bank:Checking"
  openai_prompt_type: default

Bank Credit:
  beancount_account: "Liabilities:CreditCards:Bank"
  openai_prompt_type: default
